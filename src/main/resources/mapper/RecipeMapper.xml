<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.hart.mapper.RecipeMapper">

	<select id="recipelist" resultType="com.hart.domain.RecipeVO">
		select *
		from recipe
	</select>


<!-- 디테일 조인 -->
	<select id="recipeDetail" resultType="com.hart.domain.RecipeVO">
	
		select * from recipe
		where rid= #{rid}


	</select>

</mapper>


